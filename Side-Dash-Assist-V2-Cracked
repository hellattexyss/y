local dashButtonLocked = false

local function notify(text)
	pcall(function()
		StarterGui:SetCore("SendNotification", {
			Title = "Side Dash Assist";
			Text = text;
			Duration = 2;
		})
	end)
end

lockButton.MouseButton1Click:Connect(function()
	dashButtonLocked = not dashButtonLocked
	if dashButtonLocked then
		lockButton.Text = "ðŸ”’ Locked"
		notify("Dash button locked in place.")
	else
		lockButton.Text = "ðŸ”“ Unlocked"
		notify("Dash button can be dragged again.")
	end
end)

local function setMainTransparency(alpha)
	mainFrame.BackgroundTransparency = alpha
	borderFrame.BackgroundTransparency = alpha
	titleLabel.TextTransparency = alpha
	authorLabel.TextTransparency = 0.28 + (alpha * 0.7)
	versionLabel.TextTransparency = alpha
	toggleButton.TextTransparency = alpha
	closeBtn.TextTransparency = alpha
	minimizeBtn.TextTransparency = alpha
end

local function fadeInMain()
	mainFrame.Visible = true
	setMainTransparency(1)
	TweenService:Create(blur, TweenInfo.new(0.3), {Size = 12}):Play()
	TweenService:Create(mainFrame, TweenInfo.new(0.3), {BackgroundTransparency = 0}):Play()
	TweenService:Create(borderFrame, TweenInfo.new(0.3), {BackgroundTransparency = 0}):Play()
	TweenService:Create(titleLabel, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
	TweenService:Create(authorLabel, TweenInfo.new(0.3), {TextTransparency = 0.28}):Play()
	TweenService:Create(versionLabel, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
	TweenService:Create(toggleButton, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
	TweenService:Create(closeBtn, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
	TweenService:Create(minimizeBtn, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
end

local function fadeOutMain(callback)
	local t1 = TweenService:Create(blur, TweenInfo.new(0.3), {Size = 0})
	local t3 = TweenService:Create(mainFrame, TweenInfo.new(0.3), {BackgroundTransparency = 1})
	local t4 = TweenService:Create(borderFrame, TweenInfo.new(0.3), {BackgroundTransparency = 1})
	local t5 = TweenService:Create(titleLabel, TweenInfo.new(0.3), {TextTransparency = 1})
	local t6 = TweenService:Create(authorLabel, TweenInfo.new(0.3), {TextTransparency = 1})
	local t7 = TweenService:Create(versionLabel, TweenInfo.new(0.3), {TextTransparency = 1})
	local t8 = TweenService:Create(toggleButton, TweenInfo.new(0.3), {TextTransparency = 1})
	local t9 = TweenService:Create(closeBtn, TweenInfo.new(0.3), {TextTransparency = 1})
	local t10 = TweenService:Create(minimizeBtn, TweenInfo.new(0.3), {TextTransparency = 1})
	t1:Play() t3:Play() t4:Play()
	t5:Play() t6:Play() t7:Play()
	t8:Play() t9:Play() t10:Play()
	t4.Completed:Connect(function()
		mainFrame.Visible = false
		if callback then callback() end
	end)
end

closeBtn.MouseButton1Click:Connect(function()
	fadeOutMain()
end)

minimizeBtn.MouseButton1Click:Connect(function()
	fadeOutMain(function()
		openButton.Visible = true
		lockButton.Visible = true
	end)
end)

openButton.MouseButton1Click:Connect(function()
	openButton.Visible = false
	lockButton.Visible = false
	fadeInMain()
end)

fadeInMain()

