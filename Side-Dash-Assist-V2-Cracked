local root = char:WaitForChild("HumanoidRootPart")
local hum = char:WaitForChild("Humanoid")

player.CharacterAdded:Connect(function(newChar)
	char = newChar
	root = newChar:WaitForChild("HumanoidRootPart")
	hum = newChar:WaitForChild("Humanoid")
end)

local RANGE = 30
local studs = 8
local F_OFFSET = 3
local B_OFFSET = 3
local BASE_CD = 0.3
local speedMult = 0.8

local function getCD()
	if speedMult <= 0.05 then
		speedMult = 0.05
	end
	return BASE_CD / speedMult
end

local MIN_DIST = 1.2
local MAX_DIST = 14
local ANIM_L = 10480796021
local ANIM_R = 10480793962
local SOUND_ID = 10480793962

local dashing = false
local animTrack = nil
local espOn = false
local m1On = false
local dashKeyOn = false
local targetNearOn = false
local selectedPlr = nil

local dashButtonLocked = false   -- <<< used by dash button + lock btn

